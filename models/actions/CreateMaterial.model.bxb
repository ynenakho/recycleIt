action (CreateMaterial) {
  description (Constructor to material)
  type(Constructor)
  collect {
    input (description) {
      type (DescriptionConcept)
      min (Optional) max (Many)
      
    }
    computed-input (image) {
      type (Image)
      min (Optional) max (One)
      compute {
        if (!exists(description)) {
          intent {
            goal: CreateImage
          }
        }
      }
    }
    
    
  }
  output (Material)
}
