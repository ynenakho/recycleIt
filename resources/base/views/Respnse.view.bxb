result-view {
  match: Response (this)
  
  message {
      template("#{raw(this.text)}")
  }
  
  render {
    image-list-of (this.image)
  }
  
  followup {
    prompt {
      dialog (Would you like to check on other item?)
      on-confirm {
          intent {
            goal: Response
          }
      }
      on-deny {
        message (Thank you for making inpact to make our World a Better Place!)
      }
    }
  }
}